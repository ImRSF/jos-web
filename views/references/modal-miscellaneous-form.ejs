<div class="modal fade" id="modal-miscellaneous-form" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
    aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <form method="POST" action="/misc/miscellaneous" enctype="multipart/form-data">
            <div class="modal-content">
                <div class="modal-header text-center">
                    <h4 class="modal-title w-100 font-weight-bold">
                        Add Miscellaneous Item
                    </h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                        <div class="form-group row">
                            <input type="hidden" name="itemId" id="inph-itemId">
                            <label for="inpt-itemName" class="col-sm-3 col-form-label">Item Name</label>
                            <div class="col-sm-9">
                                <input type="text" name="itemName" class="form-control" id="inpt-itemName" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inpt-itemDescription" class="col-sm-3 col-form-label">Item Description</label>
                            <div class="col-sm-9">
                                <textarea name="itemDescription" class="form-control"  id="txt-itemDescription" required></textarea>
                            </div>
                        </div>
                        <!-- <div class="form-group row">
                            <label for="sel-subCategory" class="col-sm-3 col-form-label">Category</label>
                            <div class="col-sm-9">
                                <select type="text" class="form-control" name="itemCategory" id="sel-subCategory"></select>
                            </div>
                        </div> -->
                        <div class="form-group row">
                            <div class="col-sm-6">
                                <button type="submit" class="btn-small btn-primary form-control" id="btn-submit-miscellaneous">Add Misc Item
                                    </button>
                            </div>
                            <div class="col-sm-6">
                                <button type="button" data-dismiss="modal" class="btn-small btn-danger form-control">Cancel
                                    </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>