<!-- Central Modal Medium Warning -->
<div class="modal fade" id="modal-confirmDeleteNetwork" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
  aria-hidden="true">
  <div class="modal-dialog modal-notify modal-danger" role="document">
    <form method="POST" action="/misc/delete/network">
        <!--Content-->
        <div class="modal-content">
        <!--Header-->
        <div class="modal-header">
            <p class="heading lead">Modal Warning</p>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true" class="white-text">&times;</span>
            </button>
        </div>

        <!--Body-->
        <div class="modal-body">
            <div class="text-center">
            <i class="fas fa-trash fa-4x mb-3 animated rotateIn"></i>
            <p>This action will delete the selected item. Are you sure you want to continue?</p>
            </div>
        </div>

        <!--Footer-->
        <div class="modal-footer justify-content-center">
            <input type="hidden" name="networkId" id="inph-deleteNetworkId">
            <button type="submit" class="btn btn-danger">Confirm Delete</button>
            <button type="button" class="btn btn-primary" data-dismiss="modal">Go Back</button>
        </div>
        </div>
        <!--/.Content-->
    </form>
  </div>
</div>
<!-- Central Modal Medium Warning-->